      "target": "api::client.client",
      "inversedBy": "services"
    },
    "active": {
      "type": "boolean",
      "default": false
    },
    "serviceCost": {
      "type": "biginteger"
    },
    "date": {
      "type": "date"
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location.location",
      "inversedBy": "services"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "pending",
        "cancelled"
      ]
    },
    "claimed": {
      "type": "boolean",
      "default": false
    },
    "dateClaimed": {
      "type": "datetime"
    },
    "completed": {
      "type": "boolean",
      "default": false
    },
    "consecutive": {
      "type": "biginteger"
    },
